<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorDenPress" />
            <corners android:radius="@dimen/_10sdp" />
            <stroke android:width="@dimen/_1sdp" android:color="@color/colorDenPress" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/colorDen" />
            <corners android:radius="@dimen/_10sdp" />
            <gradient android:endColor="@color/colorDenMo" android:gradientRadius="25dp" android:startColor="@color/colorDen" />
        </shape>
    </item>
</selector>